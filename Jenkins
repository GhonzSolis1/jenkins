import groovy.json.JsonSlurperClassic

def jsonParse(def json) {
    new groovy.json.JsonSlurperClassic().parseText(json)
}
pipeline {
  agent { label 'master' }
  environment {
    appName = "variable" 
  }
  stages {

 stage("paso 1"){
     
      steps {
          script {			
           bat "echo 'hola mundo'" 
        }
      }
    }
	
	stage("build"){
		steps{
		script{
			bat "echo 'mi stage'"
		}
		}
  }
  
  post {
      always {          
           bat  "echo 'fase always'" 
      }
      success {
            bat  "echo 'fase success'"
        }

      failure {
           bat  "echo 'fase failure'" 
      }
      
  }
}  
